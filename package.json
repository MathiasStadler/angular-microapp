{
  "name": "shell",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "live-server dist/shell",
    "build": "npm run build:shell && npm run build:clients",
    "build:clients": "npm run build:client-a && npm run build:client-b && npm run build:client-c ",
    "build:client-a": "ng build --prod --project client-a --output-hashing none --vendor-chunk false --output-path dist/shell/client-a",
    "build:client-b": "ng build --prod --project client-b --output-hashing none --vendor-chunk false --output-path dist/shell/client-b",
    "build:client-c": "ng build --prod --project client-c --output-hashing none --vendor-chunk false --output-path dist/shell/client-c",
    "build:shell": "ng build --project shell --prod",
    "build:clients:webpack:old-solution": "webpack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngsw-config": "ngsw-config dist/shell ngsw-config.json",
    "build-pwa": "npm run build:shell && npm run build:clients && npm run ngsw-config && npm run build-ngsw",
    "start-pwa": "http-server -c-1 dist/shell -p 8765",
    "build-ngsw": "projects/service-worker/build.sh",
    "copy-idb-js": "copyfiles -f projects/service-worker/worker/src/**/*js dist/worker-es2017/src"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.9",
    "@angular/common": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/elements": "^6.1.9",
    "@angular/forms": "^6.1.9",
    "@angular/http": "^6.1.9",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/pwa": "^0.8.4",
    "@angular/router": "^6.1.9",
    "@angular/service-worker": "^6.1.9",
    "@webcomponents/custom-elements": "^1.2.1",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.7",
    "idb": "^2.1.3",
    "rxjs": "6.3.3",
    "service-worker": "0.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "^6.2.4",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/language-service": "^6.1.9",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.4",
    "@types/node": "~10.11.3",
    "codelyzer": "~4.4.4",
    "copyfiles": "^2.1.0",
    "cpr": "^3.0.1",
    "css-loader": "^1.0.0",
    "html-loader": "^0.5.5",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "live-server": "^1.2.0",
    "ngx-build-plus": "^6.1.0",
    "protractor": "^5.4.1",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript2": "^0.17.1",
    "rollup-plugin-uglify": "^4.0.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "^2.9.2",
    "webpack-cli": "^3.1.2"
  }
}
